{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "youbike-grabber",
  "main": "index.ts",
  "compatibility_date": "2025-07-26",
  "observability": {
    "enabled": true
  },
  "placement": {
    "mode": "smart"
  },
  "triggers": {
    "crons": [
      "* * * * *",
      "0 * * * *"
    ]
  },
  "vars": {
    "SKIP_DEPENDENCY_INSTALL": "true"
  }
}
