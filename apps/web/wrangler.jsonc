{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "youbike-web",
  "main": "./dist/_worker.js/index.js",
  "compatibility_date": "2025-07-26",
  "compatibility_flags": ["nodejs_compat"],
  "workers_dev": false,
  "observability": {
    "enabled": true
  },
  "placement": {
    "mode": "smart"
  },
  "assets": {
    "binding": "ASSETS",
    "directory": "./dist"
  }
}
