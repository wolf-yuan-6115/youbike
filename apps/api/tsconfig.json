{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "strict": true,
    "skipLibCheck": true,
    "types": ["node", "bun"],
    "jsx": "react-jsx",
    "outDir": "./dist",
    "maxNodeModuleJsDepth": 15,
    "skipDefaultLibCheck": true
  },
  "exclude": ["node_modules"]
}
